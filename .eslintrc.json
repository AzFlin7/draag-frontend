{
  "root": true,
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 12,
    "ecmaFeatures": {
      "jsx": true,
      "modules": true,
      "experimentalObjectRestSpread": true
    }
  },
  "extends": [
    "eslint:recommended",
    "prettier"
  ],
  "env": {
    "es2021": true,
    "node": true
  },
  "rules": {
    "no-console": "off",
    "no-unused-vars": "off",
    "no-undef": "off"
  }
}